pipeline {

  agent any

  stages {

    stage('Checkout Source') {
      steps {
        git 'https://github.com/niralaakkan/docker_nginx.git'
      }
    }

    stage('Deploy App') {
      steps {
        script {
          kubernetesDeploy(configs: "ngnix_deployment.yaml", kubeconfigId: "kubeconfig")
        }
      }
    }

  }

}