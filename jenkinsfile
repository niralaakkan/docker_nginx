pipeline {

  agent { label 'kubepods' }

  stages {

    stage('Checkout Source') {
      steps {
        git url:'https://github.com/niralaakkan/docker_nginx.git'
      }
    }

    stage('Deploy App') {
      steps {
        script {
          kubernetesDeploy(configs: "ngnix_deployment.yaml", kubeconfigId: "mykubeconfig")
        }
      }
    }

  }

}